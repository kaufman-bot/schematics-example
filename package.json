    "@nrwl/cli": "13.8.1",
    "@nrwl/eslint-plugin-nx": "13.8.1",
    "@nrwl/jest": "13.8.1",
    "@nrwl/linter": "13.8.1",
    "@nrwl/nest": "^13.8.1",
    "@nrwl/node": "13.8.1",
    "@nrwl/tao": "13.8.1",
    "@nrwl/workspace": "13.8.1",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "~5.10.0",
    "@typescript-eslint/parser": "~5.10.0",
    "eslint": "~8.7.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "27.2.3",
    "nx": "^13.8.1",
    "prettier": "^2.5.1",
    "rucken": "^4.4.4",
    "source-map-support": "^0.5.21",
    "ts-jest": "27.0.5",
    "typescript": "~4.6.4"
  },
  "dependencies": {
    "@kaufman-bot/bot-in-groups-server": "*",
    "@kaufman-bot/core-server": "*",
    "@kaufman-bot/debug-messages-server": "*",
    "@kaufman-bot/facts-generator-server": "*",
    "@kaufman-bot/html-scraper-server": "*",
    "@kaufman-bot/language-swither-server": "*",
